{
    "job": [
      {
        "id": 1,
        "type": "download-file",
        "inputs": [
            "http://nginx/sample.tif",
            "/opt/geoserver_data/20220101T0000.tif"
          ]
      },
      {
        "id": 2,
        "type": "geotiff-validator",
        "inputs": [
          {
            "outputOfId": 1,
            "outputIndex": 0
          }
        ]
      },
      {
        "id": 3,
        "type": "geoserver-create-imagemosaic-datastore",
        "inputs": [
            "langenfeld",
            "langenfeld_temperature"
          ]
      },
      {
        "id": 4,
        "type": "geoserver-publish-imagemosaic",
        "inputs": [
            "langenfeld",
            "langenfeld_temperature",
            {
              "outputOfId": 1,
              "outputIndex": 0
            },
            true
          ]
      }
    ]
}
